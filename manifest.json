{
  "manifest_version": 2,

  "name": "Smarter Duo",
  "description": "This extension makes Duolingo's choice of practice more intelligent",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://www.duolingo.com/api/1/"
  ]
}
